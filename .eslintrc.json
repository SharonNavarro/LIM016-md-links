{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-shadow": "off",
        "max-len":"off",
        "no-trailing-spaces":"off",
        "spaced-comment":"off",
        "no-console":"off",
        "no-else-return": "off"
    }
}
